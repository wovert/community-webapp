{
  "name": "community-webapp",
  "version": "1.0.0",
  "description": "- SEO不友好\r - 加载首屏时间较长，用户体验差",
  "main": "index.js",
  "scripts": {
    "dev-client:build": "webpack --mode=development --config build/webpack.config.client.js",
    "dev-server:build": "webpack --mode=development --config build/webpack.config.server.js",
    "dev:build": "yarn clear && yarn dev-client:build && yarn dev-server:build",
    "dev:client": "yarn clear && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "prod:server": "cross-env NODE_ENV=production nodemon --exec babel-node server/server.js",
    "dev:server": "nodemon --exec babel-node server/server.js",
    "clear": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wovert/community-webapp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wovert/community-webapp/issues"
  },
  "homepage": "https://github.com/wovert/community-webapp#readme",
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "axios": "^0.18.0",
    "express": "^4.17.0",
    "react": "^16.8.6",
    "react-dom": "npm:@hot-loader/react-dom",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "memory-fs": "^0.4.1",
    "react-hot-loader": "^4.8.6",
    "rimraf": "^2.6.3",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  }
}
